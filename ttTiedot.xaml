<Window x:Class="Varasto.ttTiedot"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Varasto"
        mc:Ignorable="d"
        Title="Työntekijät" Height="450" Width="600" Background="Black">
    <Grid>
        <Border Margin="20" Background="Gray">
            <StackPanel>
                <DataGrid AutoGenerateColumns="True" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Name="ttTietoPankki" Margin="10" FontSize="15"></DataGrid>
                <Separator></Separator>
                <Button Width="100" Height="20" HorizontalAlignment="Left" Name="btnAddTT" Content="Lisää Työntekijä" Click="btnAddTT_Click"></Button>
            </StackPanel>
        </Border>
            
    </Grid>
</Window>
