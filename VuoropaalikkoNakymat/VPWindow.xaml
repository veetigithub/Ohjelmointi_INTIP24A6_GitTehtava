        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Varasto"
        mc:Ignorable="d"
        Title="Varastopäällikkö" Height="450" Width="800">
    <Grid RenderTransformOrigin="0.491,0.55">
        <ListBox Name="menuList" SelectionChanged="MenuList_SelectionChanged">
            <ListBoxItem>Keräily</ListBoxItem>
            <ListBoxItem>Hyllytys</ListBoxItem>
            <ListBoxItem>Tuotteiden poisto</ListBoxItem>
            <ListBoxItem>Tuotteiden lisäys</ListBoxItem>
            <ListBoxItem>Inventaario</ListBoxItem>
            <ListBoxItem Selected="ListBoxItem_Selected">Työntekijät</ListBoxItem>
        </ListBox>
    </Grid>
</Window>
